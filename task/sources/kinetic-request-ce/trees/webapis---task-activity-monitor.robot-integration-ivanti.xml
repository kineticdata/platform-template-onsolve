formatted_end_date = query_end_time.iso8601

query = JSON.parse(@results['Retrieve Integration Config']['Values JSON'])['Qualification']

query.gsub!( "{{Start Date}}", formatted_start_date )
query.gsub!( "{{End Date}}", formatted_end_date )

query

