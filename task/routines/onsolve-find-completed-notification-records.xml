      <visible>false</visible>
      <parameters>
            <parameter id="UUIDs JSON" label="UUIDs JSON" menu="" required="true" tooltip="A JSON object of open MIR3 Notification UUIDs">&lt;%=
uuids = []

open_records = JSON.parse( @results['Retrieve Submission records']['Result'] )

open_records.each{| record |
  uuids.push( record['values']['Onsolve Id'] )
}

uuids.to_json
%&gt;</parameter>
