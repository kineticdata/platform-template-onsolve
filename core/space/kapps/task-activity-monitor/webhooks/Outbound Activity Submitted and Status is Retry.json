{
  "authStrategy": {
    "type": "Signature",
    "properties": [
      {
        "name": "Key",
        "value": "kinops-request-ce"
      },
      {
        "name": "Secret",
        "sensitive": "true"
      }
    ]
  },
  "event": "Updated",
  "filter": "form('slug') === \"outbound-activity\" && values('Status') === \"Retry\"",
  "name": "Outbound Activity Submitted and Status is Retry",
  "type": "Submission",
  "url": "/app/api/v1/run-tree/${space('attribute:Task Source Name')}/Submissions%20%3E%20${kapp('slug')}%20%3E%20${form('slug')}/Submitted%20or%20Updated",
  "attributes": null
}